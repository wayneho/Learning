<h1> Users#index </h1>

<% @users.each do |user| %>
	Username: <%= link_to  "#{user.username}", user_path(user) %>
	<%= link_to "Edit", edit_user_path(user.id) %> 
	<%= link_to "Delete", user_path(user), method: :delete, data: {confirm: "Really delete the user?"} %><br />
<% end %>

<%= link_to "New User", new_user_path %>
